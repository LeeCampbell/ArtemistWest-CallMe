<UserControl x:Class="ArtemisWest.CallMe.Shell.UI.Contact.ContactSearchResultsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Contacts="clr-namespace:ArtemisWest.CallMe.Contract.Contacts;assembly=ArtemisWest.CallMe.Contract">
    <Control.Resources>
        
    </Control.Resources>
    <StackPanel>
        <TextBlock Text="Contacts goes here"/>
        <ItemsControl ItemsSource="{Binding Contacts}">
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <StackPanel>
                        <TextBlock Text="{Binding Title}"/>
                        <Image Source="{Binding Image}"/>
                    </StackPanel>
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
    </StackPanel>
</UserControl>
